w = 2 * pi * (w/length(musicSpectrum1));
dataplot = plot(w, dataSpectrum1);
hold on;
musicplot = plot(w, musicSpectrum1);
hold off;
axis tight;
title('Frequency spectrum using MUSIC(red) and original');
set(musicplot, 'Color', 'r');

